{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "swodlr-jobset",
  "type": "object",
  "properties": {
    "jobs": {
      "type": "array",
      "items": {
        "$ref": "swodlr-job#"
      }
    },
    "inputs": {
      "type": "array",
      "items": {
        "$ref": "swodlr-input#"
      }
    },
    "waiting": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": [
    "jobs"
  ]
}
